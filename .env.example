# Environment Configuration for Berlin-Hamburg Corridor Analysis

# Database Configuration
DATABASE_URL=postgresql://corridor_user:corridor_pass@localhost:5432/corridor_analysis
DB_HOST=localhost
DB_PORT=5432
DB_NAME=corridor_analysis
DB_USER=corridor_user
DB_PASSWORD=corridor_pass

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379

# Server Configuration
PORT=3000
NODE_ENV=development

# API Keys (obtain from Deutsche Bahn)
STADA_API_KEY=your_stada_api_key_here
TIMETABLES_API_KEY=your_timetables_api_key_here

# Logging
LOG_LEVEL=info

# Cache Configuration
CACHE_TTL=3600
ANALYSIS_CACHE_TTL=1800

# Corridor Configuration
CORRIDOR_START_STATION=8011160  # Berlin Hbf EVA
CORRIDOR_END_STATION=8002548    # Hamburg Hbf EVA